{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": false,
    "module": "ESNext",
    "target": "ES2019",
    "moduleResolution": "bundler",
    "rootDir": "./src",
    "jsx": "react-jsx",
    "allowJs": false,
    "noEmit": false,
    "incremental": false,
    "composite": false
  },
  "include": ["src"]
}
